@model IEnumerable<CloudyShop.Models.EF.Order>
@if (Model != null)
{
    var i = 1;
    <h2>Lịch sử đơn hàng</h2>
    <table class="table table-bordered">
        <thead>
            <tr>
                <td>STT</td>
                <td>Mã DH</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@i</td>
                    <td>@item.Code</td>
                    <td><a href="/review/view/@item.Id">Xem chi tiết</a></td>
                </tr>
                i++;
            }
        </tbody>
    </table>
}
